        self.birth_year = birth_year

    def get_course(self):
        if self.birth_year is None:
            return None 
        current_year = 2024
        entrance_year = self.birth_year + 15  
        course = current_year - entrance_year + 1
        return course if 1 <= course <= 4 else None  

